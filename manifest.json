{
  "name": "@deskpro-apps/logmein-rescue",
  "title": "LogMeIn Rescue",
  "description": "Create instant remote session from a ticket and share them with your users who need support.",
  "version": "0.0.3",
  "scope": "agent",
  "isSingleInstall": false,
  "hasDevMode": true,
  "serveUrl": "https://apps-cdn.deskpro-service.com/__name__/__version__",
  "targets": [{ "target": "ticket_sidebar", "entrypoint": "index.html" }],
  "settings": {},
  "proxy": {
    "whitelist": [
      {
        "url": "https://secure.logmeinrescue.com/api/.*",
        "methods": ["POST"],
        "timeout": 20
      }
    ]
  }
}
